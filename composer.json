{
  "name": "kabudu/symfony-autowire-test-bundle",
  "type": "package",
  "description": "Symfony Autowire test bundle",
  "authors": [
    {
      "name": "kabudu",
      "email": "kabudu@example.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "symfony/console": "v4.1.0",
    "symfony/http-kernel": "v4.1.0",
    "symfony/dependency-injection": "v4.1.0",
    "symfony/config": "v4.1.0"
  },
  "require-dev": {
    "roave/security-advisories":"dev-master",
    "phpunit/phpunit": "6.*"
  },
  "repositories": [

  ],
  "autoload": {
    "psr-4": {
      "Symfony\\Bundle\\CustomBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
